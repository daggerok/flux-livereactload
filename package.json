{
  "name": "flux-livereactload",
  "version": "1.0.0",
  "description": "babel es2015 reflux jsx react browserify watchify livereactload",
  "main": "boundle.js",
  "scripts": {
    "clean": "rm -rf dist/",
    "clear": "rm -rf dist/ node_modules/",
    "prestart": "npm install --no-optional",
    "postinstall": "npm run vendors",
    "vendors": "mkdir -pv dist/ && cp -Rf node_modules/jquery/dist/jquery.min.js dist/ && cp -Rf node_modules/bootstrap/dist/js/bootstrap.min.js dist/ && cp -Rf node_modules/bootstrap/dist/css/bootstrap.min.css dist/ && cp -Rf api dist/",
    "start": "watchify src/entry.jsx -o 'envify > dist/bundle.js' -t babelify -p livereactload -d -v --delay 200 --fast=true",
    "prebuild": "npm run clean && npm install --no-optional",
    "build": "browserify src/entry.jsx -o dist/bundle.js -t babelify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daggerok/flux-livereactload.git"
  },
  "keywords": [
    "babel",
    "browserify",
    "es2015",
    "reflux",
    "jsx",
    "livereactload",
    "react",
    "watchify"
  ],
  "author": "Maksim Kostromin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/daggerok/flux-livereactload/issues"
  },
  "homepage": "https://github.com/daggerok/flux-livereactload#readme",
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "livereactload": "^2.1.0",
    "react-proxy": "^2.0.0",
    "watchify": "^3.6.1"
  },
  "dependencies": {
    "bootstrap": "^3.3.6",
    "jquery": "^2.1.4",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "reflux": "^0.3.0",
    "superagent": "^1.6.1"
  }
}
